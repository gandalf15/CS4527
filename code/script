docker-compose -f docker-compose-simple.yaml up

docker exec -it chaincode bash

CORE_PEER_ADDRESS=peer:7051 CORE_CHAINCODE_ID_NAME=mycc:0 ./ipsc/chaincode/chaincode

docker exec -it cli bash

peer chaincode install -p chaincode/ipsc/chaincode -n mycc -v 0

peer chaincode instantiate -n mycc -v 0 -c '{"Args":["a","10"]}' -C myc

peer chaincode invoke -n mycc -c '{"Args":["putDataEntry", "1", "test data", "50", "Celsius", "20180321163750", "marcel"]}' -C myc

peer chaincode query -n mycc -c '{"Args":["getDataEntryById","1"]}' -C myc

peer chaincode query -n mycc -c '{"Args":["queryDataEntryByPublisher","marcel"]}' -C myc

docker-compose -f docker-compose-simple.yaml down
